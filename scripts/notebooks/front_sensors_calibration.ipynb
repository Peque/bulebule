   "source": [
    "df['reference_distance'] = df.index.values\n",
    "df['left_distance_error'] = df['left_distance'] - df['reference_distance']\n",
    "df['right_distance_error'] = df['right_distance'] - df['reference_distance']\n",
    "df['zero_distance_error'] = 0"
